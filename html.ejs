<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <thead>
            <% header.forEach(column => {%>
                <th><%= column %></th>
      <%})%>   
        </thead>
         <% rows.forEach(row => {%>
            <tr>
                <% row.forEach(element =>{ %>
                <td><%= element %></td> 
                <% }) %>  
            </tr>
         <% }) %>  
    </table>
   
</body>
</html>